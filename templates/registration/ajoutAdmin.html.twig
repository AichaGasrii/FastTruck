
{% block body %}
    <h1>Register</h1>

    {{ form_start(ajoutAdmin) }}

    {{ form_row(ajoutAdmin.nom) }}
    {{ form_row(ajoutAdmin.prenom) }}
    {{ form_row(ajoutAdmin.email) }}
    {{ form_row(ajoutAdmin.plainPassword, {
        label: 'Password'
    }) }}

    <button type="submit" class="btn">Register</button>
    {{ form_end(ajoutAdmin) }}
{% endblock %}